{
	"schemaVersion": 1,
	"id": "etherlink",
	"version": "${version}",
	"name": "EtherLink",
	"description": "A mod that links a Fabric MC server to a Discord bot!",
	"authors": [
		"Trev!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/"
	},
	"license": "CC0-1.0",
	"icon": "assets/modid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.etherlink.EtherLink"
		],
		"client": [
			"com.etherlink.EtherLinkClient"
		]
	},
	"mixins": [
		"modid.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}